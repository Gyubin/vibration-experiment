<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="io.qbinson.vibration.MainActivity"
    tools:showIn="@layout/app_bar_main">


    <EditText
        android:layout_width="230dp"
        android:layout_height="66dp"
        android:inputType="numberPassword"
        android:ems="10"
        android:id="@+id/editText"
        android:enabled="false"
        android:layout_below="@+id/button_none"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp" />

    <Button
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:text="중단"
        android:id="@+id/button_cancel"
        android:onClick="mOnClick"
        android:background="#4a4245"
        android:textColor="#000000"
        android:textSize="20dp"
        android:enabled="false"
        android:layout_alignTop="@+id/editText"
        android:layout_alignEnd="@+id/button_strong" />


    <Button android:id="@+id/button1"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:text="CELL 1"
        android:onClick="mOnClick"
        android:background="#f4407d"
        android:textColor="#ffffff"
        android:textSize="30dp" />

    <Button
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:text="CELL 2"
        android:id="@+id/button2"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:onClick="mOnClick"
        android:background="#f4407d"
        android:textColor="#ffffff"
        android:textSize="30dp" />

    <Button
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:text="CELL 3"
        android:id="@+id/button3"
        android:onClick="mOnClick"
        android:background="#f4407d"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:layout_alignTop="@+id/button4"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:text="CELL 4"
        android:id="@+id/button4"
        android:onClick="mOnClick"
        android:background="#f4407d"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:layout_below="@+id/button2"
        android:layout_alignStart="@+id/button2"
        android:layout_marginTop="15dp" />

    <Button
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:text="CELL 5"
        android:id="@+id/button5"
        android:onClick="mOnClick"
        android:background="#f4407d"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:layout_alignTop="@+id/button6"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:text="CELL 6"
        android:id="@+id/button6"
        android:onClick="mOnClick"
        android:background="#f4407d"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:layout_below="@+id/button4"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp" />

    <Button
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="none"
        android:id="@+id/button_none"
        android:onClick="mOnClick"
        android:layout_below="@+id/button5"
        android:layout_alignParentStart="true"
        android:layout_marginTop="15dp" />

    <Button
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="weak"
        android:id="@+id/button_weak"
        android:onClick="mOnClick"
        android:layout_alignTop="@+id/button_none"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="strong"
        android:id="@+id/button_strong"
        android:onClick="mOnClick"
        android:layout_alignTop="@+id/button_weak"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
